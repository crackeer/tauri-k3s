{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:default",
    "fs:allow-app-write",
    "fs:allow-app-write-recursive",
    "fs:allow-app-read-recursive",
    "fs:create-app-specific-dirs",
    "clipboard-manager:allow-write-text",
    "sql:default",
    "sql:allow-execute",
    "sql:allow-select"
  ]
}